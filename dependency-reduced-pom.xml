<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.isaac.word2vecf</groupId>
  <artifactId>Word2VecfJava</artifactId>
  <name>Word2VecfJava</name>
  <version>0.0.1-SNAPSHOT</version>
  <url>http://maven.apache.org</url>
  <build>
    <finalName>Word2VecfJava</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.3</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <mainClass>com.isaac.examples</mainClass>
                </transformer>
              </transformers>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <archive>
            <manifest>
              <addClasspath>true</addClasspath>
              <classpathPrefix>lib/</classpathPrefix>
              <mainClass>com.isaac.examples</mainClass>
            </manifest>
          </archive>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <properties>
    <junidecode.version>0.1.1</junidecode.version>
    <commons-io.version>2.4</commons-io.version>
    <slf4j.version>1.7.21</slf4j.version>
    <stanfordcorenlp.version>3.6.0</stanfordcorenlp.version>
    <commons-lang.version>3.5</commons-lang.version>
    <guava.version>21.0</guava.version>
    <nd4j.version>0.8.0</nd4j.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <jwi.version>2.2.3</jwi.version>
    <commons-math.version>3.6.1</commons-math.version>
  </properties>
</project>

